<%= form_for @campground do |f| %>
    <div>
        <%= f.text_field :picture, placeholder: 'Picture' %>
    </div>
    <div>
        <%= f.text_field :name, placeholder: 'Name' %>
    </div>
    <div>
        <%= f.number_field :zip, placeholder: 'Zip Code' %> 
        <%= f.text_field :address, placeholder: 'Street Address' %> 
        <%= f.text_field :city, placeholder: 'City' %> 
        <%= f.text_field :state, placeholder: 'State' %> 
        <%= f.text_field :country, placeholder: 'country' %>
    </div>
    <div>
        <%= f.text_area :description, placeholder: 'Description' %>
    </div>
    <div>
        <%= f.submit 'Save' %>
    </div>
<% end %>