<%= link_to 'New', new_course_path %>
<% @courses.each do |course| %>
    <div>
        <h4><%= course.name %></h4>
        <h5><%= course.location %></h5>
        <p>
            <%= course.description %> 
        </p>

        <%= link_to 'Edit', edit_course_path(course) %>
        <%= link_to 'Delete', course_path(course), :method => :destroy%>
        <%= link_to 'Show', course_path(course) %>
    </div>
<% end %>